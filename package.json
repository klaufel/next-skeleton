{
  "name": "next-skeleton",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=16",
    "pnpm": ">=7"
  },
  "workspaces": [
    "components",
    "literals"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "components:generate": "clv-components generate",
    "literals:check": "clv-literals check",
    "phoenix": "pnpm -r exec -- pnpm dlx rimraf node_modules && pnpm dlx rimraf node_modules pnpm-lock.yaml && pnpm install"
  },
  "dependencies": {
    "@clv/components": "workspace:*",
    "@clv/literals": "workspace:*",
    "next": "12.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/node": "18.0.3",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "eslint": "8.19.0",
    "eslint-config-next": "12.2.0",
    "prettier": "2.7.1",
    "typescript": "4.7.4"
  }
}
