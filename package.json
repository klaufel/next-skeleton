{
  "name": "next-skeleton",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "pnpm --filter @clv/app dev",
    "build": "pnpm --filter @clv/app build",
    "start": "pnpm --filter @clv/app start",
    "components:generate": "pnpm --filter @clv/literals components:generate",
    "literals:check": "pnpm --filter @clv/literals literals:check",
