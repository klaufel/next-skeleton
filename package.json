{
  "name": "next-skeleton",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=16",
    "pnpm": ">=7"
  },
  "workspaces": [
    "components",
    "literals"
  ],
  "scripts": {
    "dev": "cd app && pnpm run dev",
    "build": "cd app && pnpm run build",
    "start": "cd app && pnpm run start",
    "components:generate": "clv-components generate",
    "literals:check": "clv-literals check",
    "phoenix": "pnpm -r exec -- pnpm dlx rimraf node_modules && pnpm dlx rimraf node_modules pnpm-lock.yaml && pnpm install"
  },
  "devDependencies": {
    "@types/node": "18.0.3",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "eslint": "8.19.0",
    "eslint-config-next": "12.2.0",
    "prettier": "2.7.1",
    "typescript": "4.7.4"
  }
}
